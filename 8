public class Adder{
    public static int adder(int a,int b){
        return(a+b);
    }
    public static int adder(int a,int b, int c){
        return(a+b+c);
        }
        
        public static void main(String[] args){
            System.out.print(adder(3,5));
            System.out.println(" ");
            System.out.print(adder(3,9,7));
        }
    }
